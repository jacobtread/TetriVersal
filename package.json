{
  "name": "tetriversal",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:ts": "ts-node app/app.ts",
    "start": "npm run build && node build/app.js",
    "build": "tsc",
    "compile": "npm run build && pkg build/app.js -t node14-linux-x64,node14-win-x64 --out-path bin",
    "compile:client": "npm run build && pkg build/clientApp.js -t node14-linux-x64,node14-win-x64 --out-path bin"
  },
  "author": "Jacobtread <jacobtread@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.1",
    "dotenv": "^9.0.2",
    "uuid": "^8.3.2",
    "ws": "^7.4.5"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "targets": [
      "node14-linux-x64",
      "node14-win-x64"
    ],
    "outputPath": "bin"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.1",
    "@types/node": "^15.0.2",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.2",
    "pkg": "^5.1.0",
    "readline": "^1.3.0"
  }
}
